{
  "version": "1.0.0",
  "name": "Bring Biofeed module to standby (RinsingQualityBased)",
  "author": "Markus Graube <markus.graube@tu-dresden.de>",
  "initial_step": "S1.Rinsing",
  "steps": [
    {
      "name": "S1.Rinsing",
      "operations": [
        {
          "service": "RinsingQualityBased",
          "command": "start",
          "parameter": [
			{ "name": "CondLimit",
			  "value": 0.05
			}
          ]
        }
      ],
      "transitions": [
        {
          "next_step": "S1.WaitForIdle",
          "condition": {
            "type": "state",
            "service": "RinsingQualityBased",
            "state": "completed"
          }
        }
      ]
    },
	{
      "name": "S1.WaitForIdle",
      "operations": [ ],
      "transitions": [
        {
          "condition": { "type": "state", "service": "RinsingQualityBased", "state": "idle" },
          "next_step": "completed"
        }
      ]
    }
    ]
}